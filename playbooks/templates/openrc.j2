# Ansible managed
export LC_ALL=C

# COMMON OPENSTACK ENVS
export OS_ENDPOINT_TYPE=internalURL
export OS_INTERFACE=internalURL
export OS_USERNAME={{ keystone_admin_user_name }}
export OS_PASSWORD='{{ keystone_auth_admin_password }}'
export OS_PROJECT_NAME={{ keystone_admin_tenant_name }}
export OS_TENANT_NAME={{ keystone_admin_tenant_name }}
export OS_AUTH_URL={{ auth_url }}/v3
export OS_NO_CACHE={{ keystone_service_adminuri_insecure }}
export OS_USER_DOMAIN_NAME=Default
export OS_PROJECT_DOMAIN_NAME=Default
export OS_REGION_NAME=RegionOne

# For openstackclient
export OS_IDENTITY_API_VERSION=3
export OS_AUTH_VERSION=3
